{
  "compilerOptions": {

    "resolveJsonModule": true,
    "types": ["node", "express"],
    "moduleResolution": "node",
    "target": "es2016",                                 
    "module": "commonjs",                                
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                
  }
}
